<title>calci</title>
</head>
<body>
	<%
	int num = Integer.parseInt(request.getParameter("num"));
	String btn = request.getParameter("btn");
	switch (btn) {

	case "fact":
		int fact = 1;

		for (int i = 1; i <= num; i++) {
			fact = fact * i;
			
		}
		out.println("<b><center>fact: "+fact);
	}
	%>
</body>
