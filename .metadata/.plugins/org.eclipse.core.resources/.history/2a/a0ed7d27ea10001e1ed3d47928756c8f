package com.IET.beans;

public class UserInfo {
	
	private String uname;
	private String password;
	private String role;
	private String skills;
	
	public UserInfo() {
		super();
	}
	
	
	public UserTitle(String uname, String password, String role, String skills) {
		super();
		this.uname = uname;
		this.password = password;
		this.role = role;
		this.skills = skills;
	}

	

	
}
